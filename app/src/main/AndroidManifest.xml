<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.relatarproblemas">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_splash_screen"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_splash_screen"
        android:supportsRtl="true"
        android:theme="@style/Theme.RelatarProblemas">
        <activity android:name=".Notes.NotesActivity"
            android:label="Notes"/>
        <activity android:name=".Settings.SettingsActivity"
            android:label="Settings"/>
        <activity android:name=".Map.MapActivity"
            android:label="Map"/>
        <activity android:name=".Login.LoginActivity"
            android:label="Login"/>
        <activity android:name=".SplashScreen.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>